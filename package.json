{
	"name": "make-mp-data",
	"version": "2.0.24",
	"description": "builds all mixpanel primitives for a given project",
	"type": "module",
	"main": "index.js",
	"types": "types.d.ts",
	"files": [
		"index.js",
		"cli.js",
		"types.d.ts",
		"lib/",
		"dungeons/",
		"!dungeons/customers/",
		"package.json",
		"README.md"
	],
	"scripts": {
		"new:dungeon": "node ./scripts/create-dungeon.mjs",
		"ui": "node ./ui/server.js",
		"ui:dev": "nodemon ./ui/server.js",
		"start": "node ./index.js",
		"dev": "nodemon scratch.mjs --ignore ./data/*",
		"prune": "rm -f ./data/* && rm -f ./tmp/* && rm -f vscode-profile-*",
		"post": "npm publish",
		"deps": "./scripts/update-deps.sh",
		"test": "NODE_ENV=test vitest run",
		"coverage": "vitest run --coverage && open ./coverage/index.html",
		"typecheck": "tsc --noEmit",
		"typecheck:build": "tsc --noEmit --project tsconfig.build.json",
		"typecheck:strict": "echo '‚ö†Ô∏è  Running pedantic strict checks (many false positives expected)...' && tsc --noEmit --project tsconfig.build.json --strict",
		"typecheck:summary": "echo 'üîç Type-checking all shipped files...' && npm run typecheck:build && echo '‚úÖ All TypeScript checks passed!'",
		"test:quick": "node ./tests/benchmark/quick-test.mjs",
		"exp:soup": "node ./tests/testSoup.mjs",
		"func:local": "functions-framework --target=entry",
		"func:deploy": "./scripts/deploy.sh",
		"textGen": "node ./lib/templates/textQuickTest.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ak--47/make-mp-data.git"
	},
	"bin": {
		"make-mp-data": "./entry.js"
	},
	"keywords": [
		"mixpanel",
		"stream",
		"analytics",
		"tracking",
		"server",
		"CLI",
		"datamart",
		"scd 2",
		"dummy data",
		"fake data"
	],
	"author": "ak@mixpanel.com",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/ak--47/make-mp-data/issues"
	},
	"homepage": "https://github.com/ak--47/make-mp-data#readme",
	"dependencies": {
		"@google-cloud/functions-framework": "^3.4.2",
		"@google-cloud/storage": "^7.14.0",
		"ak-fetch": "^2.0.12",
		"ak-gemini": "^1.0.59",
		"ak-tools": "^1.1.12",
		"chance": "^1.1.11",
		"dayjs": "^1.11.11",
		"dotenv": "^16.4.5",
		"express": "^4.21.2",
		"google-auth-library": "^9.15.0",
		"hyparquet-writer": "^0.6.1",
		"mixpanel-import": "^3.0.0",
		"p-limit": "^3.1.0",
		"seedrandom": "^3.0.5",
		"sentiment": "^5.0.2",
		"tracery-grammar": "^2.8.4",
		"yargs": "^17.7.2"
	},
	"devDependencies": {
		"@vitest/ui": "^2.1.9",
		"nodemon": "^3.1.3",
		"typescript": "^5.6.0",
		"vitest": "^2.1.9"
	},
	"nodemonConfig": {
		"ignore": [
			"data/",
			"tmp/"
		]
	}
}
